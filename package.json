{
  "name": "delta",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "db:init": "sequelize db:create",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:undo:all && sequelize db:seed:all",
    "migrate": "sequelize db:migrate",
    "build": "babel src -d api-build",
    "lint": "eslint src",
    "doc": "apidoc -i src",
    "start": "npm run doc && babel-node ./src/index.js",
    "test": "jest"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "apidoc": {
    "title": "API Documentation",
    "sampleUrl": "https://delta-api.herokuapp.com/api"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "compression": "^1.7.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "image-thumbnail": "^1.0.3",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.1",
    "mongoose": "^5.5.8",
    "multer": "^1.4.1",
    "nodemailer": "^4.6.4",
    "pg": "^7.11.0",
    "sequelize": "^5.8.9",
    "sequelize-cli": "^5.5.0",
    "superagent": "^3.8.2",
    "watson-developer-cloud": "^3.5.3"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "apidoc": "^0.17.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "supertest": "^3.1.0"
  }
}
