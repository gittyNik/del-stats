{
  "name": "delta",
  "version": "2.1.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "db:init": "sequelize db:create",
    "db:clean": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "admin": "babel-node src/util/admin.js",
    "build": "npm run db:migrate && babel src -d api-build",
    "lint": "nodemon --watch src --exec eslint src --fix",
    "doc": "apidoc -i src",
    "start-dev": "nodemon --watch src --exec \"apidoc -i src && babel-node --inspect\" src",
    "start-dev-lite": "nodemon --watch src --exec \"babel-node \" src",
    "start-prod": "node api-build",
    "start": "if [ $NODE_ENV = 'production' ]; then npm run start-prod; else npm run start-dev; fi;",
    "bg-dev": "nodemon --watch src --exec babel-node src/background.js",
    "bg-prod": "node api-build/background.js",
    "worker": "if [ $NODE_ENV = 'production' ]; then npm run bg-prod; else npm run bg-dev; fi;",
    "test": "jest src/**/*.spec.js"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "apidoc": {
    "title": "API Documentation",
    "sampleUrl": "http://localhost:3000/api"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@octokit/rest": "^16.43.1",
    "@slack/events-api": "^2.3.0",
    "@slack/interactive-messages": "^1.4.0",
    "@slack/web-api": "^5.6.0",
    "@slack/webhook": "^5.0.2",
    "apidoc-swagger": "github:schoolofacceleratedlearning/apidoc-swagger",
    "aws-sdk": "^2.650.0",
    "bullmq": "^1.6.7",
    "compression": "^1.7.4",
    "core-js": "^3.6.2",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "faker": "^4.1.0",
    "hubspot": "^2.3.7",
    "ip": "^1.1.5",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.1",
    "mongoose": "^5.8.6",
    "msg91": "github:msg91/msg91#0f6f9cd",
    "multer": "^1.4.2",
    "nodemailer": "^4.6.4",
    "pg": "^7.17.0",
    "pg-native": "^3.0.0",
    "query-string": "^6.10.1",
    "regenerator-runtime": "^0.13.3",
    "sendotp": "github:msg91/sendotp-node#096de78",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "stopword": "^0.3.4",
    "superagent": "^3.8.2",
    "twitter": "^1.7.1",
    "uuid": "^3.3.3",
    "watson-developer-cloud": "^3.5.3"
  },
  "devDependencies": {
    "@babel/node": "^7.7.7",
    "apidoc": "^0.17.6",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "nodemon": "^1.19.4",
    "supertest": "^3.1.0"
  },
  "eslintIgnore": [
    "**/*.spec.js"
  ]
}
