{
  "name": "delta",
  "version": "2.1.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "db:init": "sequelize db:create",
    "db:clean": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "admin": "babel-node src/util/admin.js",
    "build": "npm run db:migrate && babel src -d api-build",
    "lint": "nodemon --watch src --exec eslint src --fix",
    "doc": "apidoc -i src",
    "start-dev": "nodemon --watch src --exec \"apidoc -i src && babel-node --inspect\" src",
    "start-dev-lite": "nodemon --watch src --exec \"babel-node \" src/index.js",
    "start-prod": "node api-build",
    "start": "if [ $NODE_ENV = 'production' ]; then npm run start-prod; else npm run start-dev; fi;",
    "bg-dev": "nodemon --watch src --exec babel-node src/background.js",
    "bg-prod": "node api-build/background.js",
    "worker": "if [ $NODE_ENV = 'production' ]; then npm run bg-prod; else npm run bg-dev; fi;",
    "test": "jest --coverage --colors && cd test && npx jest-coverage-badges",
    "test-dev": "jest --colors --watch",
    "pretest": "npm run db:seed"
  },
  "engines": {
    "node": "14.15.0",
    "npm": "6.14.10"
  },
  "apidoc": {
    "title": "API Documentation",
    "sampleUrl": "http://localhost:3000/api"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.ts --fix",
      "git add",
      "jest --bail --coverage --findRelatedTests --colors"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@octokit/plugin-retry": "^3.0.7",
    "@octokit/rest": "^18.1.0",
    "@slack/events-api": "^3.0.0",
    "@slack/interactive-messages": "^2.0.0",
    "@slack/web-api": "^6.0.0",
    "@slack/webhook": "^6.0.0",
    "apidoc-swagger": "github:schoolofacceleratedlearning/apidoc-swagger",
    "async": "^3.2.0",
    "aws-sdk": "^2.841.0",
    "babel-jest": "^26.6.3",
    "bullmq": "^1.14.4",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "faker": "^5.4.0",
    "googleapis": "^67.0.0",
    "hubspot": "^2.3.14",
    "ioredis": "^4.22.0",
    "ip": "^1.1.5",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.11.15",
    "msg91": "github:msg91/msg91#0f6f9cd",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-native": "^3.0.0",
    "pino": "^6.11.1",
    "pino-pretty": "^4.5.0",
    "query-string": "^6.14.0",
    "regenerator-runtime": "^0.13.7",
    "rollbar": "^2.20.0",
    "sendotp": "github:msg91/sendotp-node#096de78",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "stopword": "^1.0.6",
    "superagent": "^6.1.0",
    "twitter": "^1.7.1",
    "uuid": "^8.3.2",
    "watson-developer-cloud": "^3.5.3"
  },
  "devDependencies": {
    "@babel/node": "^7.12.13",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7"
  },
  "eslintIgnore": [
    "**/*.spec.js",
    "**/*.test.js"
  ]
}
