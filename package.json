{
  "name": "delta",
  "version": "2.1.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "db:init": "sequelize db:create",
    "db:clean": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "admin": "babel-node src/util/admin.js",
    "build": "npm run db:migrate && babel src -d api-build",
    "lint": "eslint src",
    "doc": "apidoc -i src",
    "start-dev": "nodemon --watch src --exec \"apidoc -i src && babel-node --inspect\" src",
    "start-prod": "node api-build",
    "start": "if [ $NODE_ENV = 'production' ]; then npm run start-prod; else npm run start-dev; fi;",
    "test": "jest"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "apidoc": {
    "title": "API Documentation",
    "sampleUrl": "https://delta-api.herokuapp.com/api"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@slack/events-api": "^2.3.0",
    "@slack/interactive-messages": "^1.4.0",
    "@slack/web-api": "^5.5.0",
    "@slack/webhook": "^5.0.2",
    "apidoc-swagger": "github:schoolofacceleratedlearning/apidoc-swagger",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "image-thumbnail": "^1.0.3",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.1",
    "mongoose": "^5.5.8",
    "msg91": "github:msg91/msg91#0f6f9cd",
    "multer": "^1.4.1",
    "nodemailer": "^4.6.4",
    "pg": "^7.11.0",
    "regenerator-runtime": "^0.13.3",
    "sendotp": "github:msg91/sendotp-node#096de78",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.0",
    "superagent": "^3.8.2",
    "uuid": "^3.3.2",
    "watson-developer-cloud": "^3.5.3"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "apidoc": "^0.17.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1",
    "supertest": "^3.1.0"
  },
  "eslintIgnore": [
    "**/*.spec.js"
  ]
}
